{
  "name": "amazing-pilates",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:wp": "turbo run dev --filter=wonder-pilates",
    "dev:kc": "turbo run dev --filter=k-classic-pilates",
    "lint": "turbo run lint",
