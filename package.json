    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.7.0",
    "turbo": "^2.7.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20"
  }
}
